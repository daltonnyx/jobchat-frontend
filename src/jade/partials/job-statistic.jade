- var table_data=[{img:"t-i-xu-ng.png",title:"Lê Hồng Minh",val1:18,val2:5,val3:6,val4:7},{img:"t-i-xu-ng-1.png",title:"Nguyễn Xuân Anh",val1:16,val2:7,val3:7,val4:2},{img:"images-4.png",title:"Lê Bảo Châu",val1:13,val2:6,val3:5,val4:2},{img:"t-i-xu-ng-2.png",title:"Phạm Nhật Vư...",val1:12,val2:7,val3:3,val4:2},{img:"t-i-xu-ng-3.png",title:"Trịnh Xuân Thanh",val1:9,val2:3,val3:2,val4:4},{img:"t-i-xu-ng-4.png",title:"Trịnh Văn Quyết",val1:8,val2:4,val3:1,val4:3},{img:"t-i-xu-ng.png",title:"Thân Đức Nam",val1:6,val2:3,val3:0,val4:3},{img:"t-i-xu-ng-1.png",title:"Nguyễn Cao Kỳ Duyên",val1:5,val2:2,val3:2,val4:1}]
.fly-out.has-close-button
    header.popup-header
        h3.popup-title.has-icon Tổng quan
        a.close-popup(href="#")
            img(src="./images/grupo-2.png", alt="X")
    .popup-content
        .row
            .col
                .dropdown-menu.time.collapsed
                    a.current-value(href="#")
                        span.current-value Tháng này
                        span.dropdown-icon
                            img(src="./images/forma-15.png", alt="")
                    ul.dropdown-list.hide
            .col
                .dropdown-menu.task.collapsed(style={float:"right"})
                    a.current-value(href="#")
                        span.current-value All Tasks
                        span.dropdown-icon
                            img(src="./images/forma-15.png", alt="")
                    ul.dropdown-list.hide
        .row
            .col.chart
                .line-chart.has-caption
                    img(src="./images/layer-1147.png", alt="")
                .chart-legends
                    ul.legends
                        li.legend.prepare
                            .legend-color(style={background: "#4b7ebb"})
                            .legend-caption Prepare Tab
                        li.legend.process
                            .legend-color(style={background: "#c0504d"})
                            .legend-caption Processing Tab
                        li.legend.finish
                            .legend-color(style={background: "#98b953"})
                            .legend-caption Finished Tab
        .row
            .col
                table.table.table-score
                    each row in table_data
                        tr
                            td
                                span.avatar
                                    img(src="./images/"+row.img, alt=row.title)
                            td= row.title
                            td
                                span.value-1= row.val1
                            td
                                span.value-2.color-primary= row.val2
                            td
                                span.value-3.color-danger= row.val3
                            td
                                span.value-4.color-success= row.val4

