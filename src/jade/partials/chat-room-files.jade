- var list_files=[{name:"Kinh nghiệm tán gái 4.0.pdf",by:"Lê hoàng Diệp Thảo",ext:"pdf",date:"2 giờ trước"},{name:"List học sinh cá biệt.docx",by:"Phạm Nhật Vượng",ext:"docx",date:"2 ngày trước"},{name:"16 phút full HD.rar",by:"Trinh Văn Quyết",ext:"rar",date:"7/6 15:38"},{name:"nhudn-dfkho-oo-d.png",by:"Phạm Nhật Vượng",ext:"image",date:"8/5 12:29"},{name:"auto-hack.exe",by:"Phan Văn Anh Vũ",ext:"other",date:"12/4 08:25"},{name:"Kinh nghiệm tán gái 4.0.pdf",by:"Lê hoàng Diệp Thảo",ext:"pdf",date:"11/4 08:10"},{name:"List học sinh cá biệt.docx",by:"Phạm Nhật Vượng",ext:"docx",date:"10/4 12:28"},{name:"16 phút full HD.rar",by:"Trinh Văn Quyết",ext:"rar",date:"2/4 23:38"},{name:"nhudn-dfkho-oo-d.png",by:"Phạm Nhật Vượng",ext:"image",date:"1/3 6:33"},{name:"auto-hack.exe",by:"Phan Văn Anh Vũ",ext:"other",date:"29/2 12:36"}]
.fly-out.has-close-button
    header.popup-header
        h3.popup-title File tải lên
        a.close-popup(href="#")
            img(src="./images/grupo-2.png", alt="X")
    .popup-content.file-list
        section.files-list
            form#search-file.search(action="/")
                .form-line
                    input.form-control(type="text", name="search_file", placeholder="Tìm kiếm tất cả file")
                button.search(type="submit")
                    img(src="./images/forma-1.png", alt="Search")
            .file-category
                ul.file-filter.tabs
                    li.nav-tab.active
                        a.nav-link(href="#") All
                    li.nav-tab
                        a.nav-link(href="#") Ảnh
                    li.nav-tab
                        a.nav-link(href="#") Tài liệu
            .files-list
                each file in list_files
                    .file-data
                        .file-icon
                            if file.ext == "pdf"
                                img(src="./images/capa-1.png", alt="pdf")
                            else if file.ext == "docx"
                                img(src="./images/capa-3.png", alt="docx")
                            else if file.ext == "rar"
                                img(src="./images/layer-1.png", alt="rar")
                            else if file.ext == "image"
                                img(src="./images/capa-2.png", alt="image")
                            else
                                img(src="./images/grupo-4.png", alt="other")
                        .file-name
                            a(href="#")
                                h6.title= file.name
                            p.uploaded_by
                                span.by by 
                                span.uploader= file.by
                        .file-date
                            span.uploaded_date= file.date
